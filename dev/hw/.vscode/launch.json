{
    "version": "0.2.0",
        "configurations": [
            {
                "name": "Cortex Debug",
                "cwd": "${workspaceRoot}",
                "executable": "./bin/main.elf",
                "request": "launch",
                "type": "cortex-debug",
                "servertype": "openocd",
                "svdFile": "STM32F407.svd",
                "configFiles": [
                    "./bin/stm32f4discovery.cfg"
                ],
                "swoConfig": {
                    "source": "probe",
                    "enabled": true,
                    "swoFrequency": 2000000,
                    "cpuFrequency": 180000000,
                    "decoders": [
                            {
                                    "port": 0,
                                    "label": "Output",
                                    "type": "console",
                                    "encoding":"ascii"                                                                                              
                            }
                    ]
                    
            }
            }
        ]
    }